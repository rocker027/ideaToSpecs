{
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(npm create:*)",
      "Bash(chmod:*)",
      "Bash(npm install:*)",
      "Bash(npm run dev:*)",
      "Bash(npm run build:*)",
      "Bash(node:*)",
      "Bash(pkill:*)",
      "Bash(true)",
      "Bash(mv:*)",
      "Bash(npm start)",
      "Bash(curl:*)",
      "Bash(lsof:*)",
      "Bash(telnet:*)",
      "Bash(nc:*)",
      "Bash(grep:*)",
      "Bash(ls:*)",
      "Bash(NODE_OPTIONS=\"--dns-result-order=ipv4first\" node server.js)",
      "Bash(find:*)",
      "Bash(./scripts/start.sh:*)",
      "Bash(npm run setup:db:*)",
      "Bash(gemini:*)",
      "Bash(kill:*)",
      "Bash(timeout:*)",
      "Bash(touch:*)",
      "Bash(gtimeout:*)",
      "Bash(cat:*)",
      "Bash(env)",
      "Bash(sqlite3:*)",
      "Bash(command:*)",
      "Bash(time gemini:*)",
      "Bash(npm run validate:*)",
      "Bash(cp:*)",
      "Bash(PORT=3011 npm test)",
      "Bash(npm test)",
      "Bash(rm:*)",
      "Bash(DATABASE_URL=\"file:../database/local.db\" node test-db-performance.js)",
      "Bash(NODE_ENV=development node test-websocket-memory.js)",
      "Bash(npm ls:*)"
    ],
    "deny": []
  },
  "hooks": {
    "PostToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "echo \"[$(date)] $CLAUDE_TOOL_COMMAND\" >> ./executed_commands.log"
          }
        ]
      }
    ]
  }
}